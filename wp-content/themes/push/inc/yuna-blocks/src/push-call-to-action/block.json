{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "push-blocks/push-call-to-action",
  "version": "0.1.0",
  "title": "Заклик до дії",
  "icon": "megaphone",
  "category": "yuna-block-category",
  "description": "Заклик до дії",
  "example": {},
  "attributes": {
    "anchorId": {
      "type": "string"
    },
    "topIndent": {
      "enum": ["indent-top-big", "indent-top-small"],
      "default": "indent-top-big",
      "type": "string"
    },
    "bottomIndent": {
      "enum": ["indent-bottom-big", "indent-bottom-small"],
      "default": "indent-bottom-big",
      "type": "string"
    },
    "blockTitle": {
      "type": "string"
    },
    "blockText": {
      "type": "string",
      "default": ""
    },
    "btnText": {
      "type": "string",
      "default": ""
    },
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imageId": {
      "type": "number",
      "default": 0
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    }

  },

  "supports": {
    "color": {
      "background": true,
      "text": true,
      "heading": true,
      "gradients": true
    },
    "html": false
  },
  "textdomain": "push",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}


