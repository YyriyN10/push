{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "push-blocks/push-our-team",
  "version": "0.1.0",
  "title": "Наша команда",
  "icon": "groups",
  "category": "yuna-block-category",
  "description": "Наша команда",
  "example": {},
  "attributes": {
    "anchorId": {
      "type": "string"
    },
    "topIndent": {
      "enum": ["indent-top-big", "indent-top-small"],
      "default": "indent-top-big",
      "type": "string"
    },
    "bottomIndent": {
      "enum": ["indent-bottom-big", "indent-bottom-small"],
      "default": "indent-bottom-big",
      "type": "string"
    },
    "blockTitle": {
      "type": "string"
    },
    "companyChiefName": {
      "type": "string",
      "default": ""
    },
    "companyChiefPost": {
      "type": "string",
      "default": ""
    },
    "chiefPhotoUrl": {
      "type": "string",
      "default": ""
    },
    "chiefPhotoId": {
      "type": "number",
      "default": 0
    },
    "slogan": {
      "type": "string",
      "default": ""
    },
    "blockText": {
      "type": "string",
      "default": ""
    }

  },

  "supports": {
    "color": {
      "background": true,
      "text": true,
      "heading": true,
      "gradients": true
    },
    "html": false
  },
  "textdomain": "push",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}


