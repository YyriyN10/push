{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "yuna-blocks/our-services",
  "version": "0.1.0",
  "title": "Наші послуги",
  "icon": "excerpt-view",
  "category": "yuna-block-category",
  "description": "Наші послуги",
  "example": {},
  "attributes": {
    "anchorId": {
      "type": "string"
    },
    "topIndent": {
      "enum": ["indent-top-big", "indent-top-small"],
      "default": "indent-top-big",
      "type": "string"
    },
    "bottomIndent": {
      "enum": ["indent-bottom-big", "indent-bottom-small"],
      "default": "indent-bottom-big",
      "type": "string"
    },
    "pageId": {
      "type": "number",
      "default": 0
    },
    "blockTitle": {
      "type": "string"
    },
    "callTitle": {
      "type": "string"
    },
    "callText": {
      "type": "string"
    },
    "btnColor": {
      "enum": ["golden-btn", "transparent-btn"],
      "default": "golden-btn",
      "type": "string"
    },
    "btnText": {
      "type": "string"
    },
    "btnAnchor": {
      "type": "string"
    }

  },

  "supports": {
    "color": {
      "background": true,
      "text": true,
      "heading": true,
      "gradients": true
    },
    "html": false
  },
  "textdomain": "yuna",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"

}


